#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/project/surveyUi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/project/surveyUi/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@3.0.9_less@4.1.3+terser@5.15.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@3.0.9_less@4.1.3+terser@5.15.0/node_modules/vite/bin/vite.js" "$@"
fi
