#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/project/surveyUi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/project/surveyUi/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vitepress-theme-demoblock@1.4.2_less@4.1.3/node_modules/vitepress-theme-demoblock/bin/vitepress-register-components.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vitepress-theme-demoblock@1.4.2_less@4.1.3/node_modules/vitepress-theme-demoblock/bin/vitepress-register-components.js" "$@"
fi
